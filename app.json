{
  "expo": {
    "name": "Home LED",
    "slug": "homeLed",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/led.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.trachex.homeLed"
    },
    "build": {
      "preview": {
        "android": {
          "buildType": "apk"
        }
      }
    },
    "ios": {
      "supportsTablet": true
    }
  }
}
